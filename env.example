# Copy this file to `.env` and fill in your credentials

# Database connection (for connecting to Docker container from host)
DATABASE_URL=postgresql://postgres:password@localhost:5432/tradebot

# Redis connection (for connecting to Docker container from host)  
REDIS_URL=redis://localhost:6379

# Trading symbols (comma-separated)
SYMBOLS=AAPL,MSFT,AMZN,GOOG,TSLA

# Alpaca API credentials (get from https://alpaca.markets)
ALPACA_KEY=your_alpaca_key_here
ALPACA_SECRET=your_alpaca_secret_here

# Polygon API key (optional, get from https://polygon.io)
POLYGON_API_KEY=your_polygon_key_here 